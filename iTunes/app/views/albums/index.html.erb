<p>List of Albums</p>

<table>
  <tr>
    <th>Title</th>
    <th>Artist</th>
    <th>Year</th>
    <th>Album Art</th>
    <th></th>
  </tr>
 
  <% @albums.each do |album| %>
    <tr>
      <td><%= album.title %></td>
      <td><%= album.artist %></td>
      <td><%= album.year %></td>
      <td><%= album.album_art %></td>
      <td><%= link_to 'Show', album_path(album) %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'New Album', new_album_path %>